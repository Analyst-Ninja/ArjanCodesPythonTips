# Flattening a 2D matrix
matrix = [[1,2],
          [3,4]]
flatten_array = [num for row in matrix for num in row]
flatten_array





transposed_matrix = []
for i in range(2):
    row = []
    for j in range(2):
        row.append(matrix[j][i])
    transposed_matrix.append(row)
transposed_matrix


transposed_matrix = [[matrix[j][i] for j in range(2)] for i in range(2)]
transposed_matrix
